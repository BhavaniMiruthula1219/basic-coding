import java.util.*;

public class Main {
    public static void main(String[] args) {
       Scanner obj = new Scanner(System.in);
      int a = obj.nextInt();
      int b = obj.nextInt();
      int c = obj.nextInt();
      int greatest = (a > b) ? ((a > c) ? a : c) : ((b > c) ? b : c);
      System.out.println(greatest);
  }
}
